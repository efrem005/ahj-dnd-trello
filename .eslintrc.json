{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "semi": "off",
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-continue": "off",
    "object-curly-newline": "off"
  }
}